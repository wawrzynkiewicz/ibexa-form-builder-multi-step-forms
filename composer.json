{
  "name": "mateuszbieniek/ezplatform-form-builder-multi-step-forms",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Adds feature to create multi-step forms in Ibexa Form Builder",
  "repositories": [
    {
      "type": "composer",
      "url": "https://updates.ibexa.co/ttl"
    }
  ],
  "autoload": {
    "psr-4": {
      "MateuszBieniek\\EzPlatformFormBuilderMultiStepFormsBundle\\": "src/bundle/",
      "MateuszBieniek\\EzPlatformFormBuilderMultiStepForms\\": "src/lib/"
    }
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "ibexa/form-builder": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^6.0",
    "ibexa/code-style": "^1.0"
  },
  "scripts": {
    "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
  }
}
